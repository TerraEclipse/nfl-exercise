{{#conferences}}
  <div class="conference row">
    <div class="col-md-12">
      <h2>{{name}}</h2>
    </div>
    {{#divisions}}
      <div class="division col-md-6">
        <h4>{{name}}</h4>
        {{#teams}}
          <div class="teams">
            <a href="/teams/{{Key}}">{{City}} {{Name}}</a>
          </div>
        {{/teams}}
      </div>
    {{/divisions}}
  </div>
{{/conferences}}